{
  "name": "react-bundle",
  "version": "1.0.1",
  "license": "BSD-3-Clause",
  "description": "Lazy load dependencies (bundle-loader or import())",
  "keywords": ["webpack", "import", "bundle-loader", "lazy", "load"],
  "repository": "github:ramitos/react-bundle",
  "main": "dist/react-bundle.umd.js",
  "jsnext:main": "dist/react-bundle.es.js",
  "module": "dist/react-bundle.es.js",
  "entry": "src/index.js",
  "scripts": {
    "lint": "eslint . --fix",
    "test": "echo 0 # bup && NODE_ENV=test jest --coverage",
    "build": "bup",
    "prepublish": "bup"
  },
  "dependencies": {
    "ispromise": "^1.1.0",
    "prop-types": "^15.5.10"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-display-name": "^6.25.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-react-jsx-self": "^6.22.0",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-preset-env": "^1.5.2",
    "bup": "^1.0.9",
    "eslint": "^3.19.0",
    "eslint-config-joyent-portal": "1.0.3",
    "eslint-config-prettier": "^2.1.1",
    "eslint-config-react-app": "^1.0.4",
    "eslint-config-xo-space": "^0.16.0",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-prettier": "^2.1.1",
    "eslint-plugin-react": "^7.0.1",
    "jest": "^20.0.4",
    "prettier": "^1.4.4",
    "react": "^15.5.4",
    "react-test-renderer": "^15.5.4"
  },
  "peerDependencies": {
    "react": "*"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testRegex": "test/index.js"
  }
}
